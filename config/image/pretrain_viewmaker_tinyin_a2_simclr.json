{
    "exp_base": null,
    "exp_name": "pretrain_viewmaker_tinyin_a1_simclr_resnet18",
    "system": "PretrainViewMakerSystem",
    "cuda": true,
    "gpu_device": 0,
    "num_gpus": 4,
    "num_nodes": 1,
    "distributed_backend": "dp",
    "seed": 2,
    "data_loader_workers": 8,
    "data_params": {
        "dataset": "tinyin-a2",
        "default_augmentations": "a2"
    },
    "model_params": {
        "out_dim": 128,
        "resnet": true,
        "resnet_version": "resnet18",
        "resnet_small": false,
