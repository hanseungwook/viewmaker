    "optim_params": {
        "batch_size": 512,
        "learning_rate": 0.03,
        "momentum": 0.9,
        "weight_decay": 1e-4,
        "validate_freq": 1,
        "patience": 1000,
        "num_view_update": 1,
        "viewmaker_optim": "adam"
    },
    "num_epochs": 1000,
    "validate": true,
    "copy_checkpoint_freq": 5,
    "continue_from_checkpoint": null
}